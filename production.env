# Configuration de production pour Annuaire Bergerac
NODE_ENV=production
VITE_APP_ENV=production
VITE_DEBUG=false

# Supabase Configuration (À configurer dans Vercel)
# VITE_SUPABASE_URL=https://votre-projet.supabase.co
# VITE_SUPABASE_ANON_KEY=votre_cle_publique
# SUPABASE_SERVICE_ROLE_KEY=votre_cle_service

# Google APIs (À configurer dans Vercel)
# VITE_GOOGLE_MAPS_API_KEY=votre_cle_google_maps
# VITE_RECAPTCHA_SITE_KEY=votre_site_key
# RECAPTCHA_SECRET_KEY=votre_secret_key

# Configuration application
VITE_APP_NAME="Annuaire Bergerac"
VITE_APP_DOMAIN="annuaire-bergerac.fr"
VITE_APP_VERSION="1.0.0"

# Features flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SENTRY=false
VITE_ENABLE_HOTJAR=false

# Cache et performance
VITE_CACHE_TTL=3600
VITE_IMAGE_CACHE_TTL=86400

# Limites
VITE_MAX_UPLOAD_SIZE=5242880
VITE_MAX_IMAGES_PER_LISTING=10
VITE_RATE_LIMIT_REQUESTS=60